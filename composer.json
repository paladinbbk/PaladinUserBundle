{
    "name": "paladinbbk/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony PaladinUserBundle",
    "keywords": ["User management"],
    "homepage": "https://github.com/paladinbbk/PaladinUserBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Chesnokov",
            "email": "dima3141592@gmail.com"
        },
        {
            "name": "VombatSoft"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "~3.0|~4.0",
        "symfony/security-bundle": "~3.0|~4.0",
        "symfony/twig-bundle": "~3.0|~4.0",
        "symfony/validator": "~3.0|~4.0",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "symfony/console": "~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Paladin\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Paladin\\UserBundle\\Tests\\": "/Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.1-dev"
        }
    }
}